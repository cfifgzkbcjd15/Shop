    @@font-face {
    font-family: adineueprocyr_boldweb;
    src: url(@Url.Content("~/Content/fonts/adineueprocyr_boldweb.ttf"));
    }

    .bgImage 
    {
    height: 400px;
    width: 100%;
    opacity: 1;
    background-image: url(@Url.Content("~/Content/img/bgImage.jpg"));
    }
</style>
</head>
<body class="adineueprocyr_b">
