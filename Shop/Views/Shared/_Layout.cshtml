    <nav class="navbar navbar-expand-lg">
        <div class="container">
        <a class="navbar-brand" href="#">
            <svg width="42" height="42" xmlns="http://www.w3.org/2000/svg">
            <g>
                <title>Layer 1</title>
                <g stroke="null" id="svg_9">
                <ellipse stroke="#000000" fill-opacity="0" stroke-width="2" ry="14.4929" rx="18.00918" id="svg_2" cy="21.41661" cx="20.726" fill="#000000"/>
                <text stroke="#000" transform="matrix(0.379141 0 0 0.402581 -22.6101 -9.01617)" font-weight="bold" font-style="normal" xml:space="preserve" text-anchor="start" font-family="sans-serif" font-size="58" stroke-width="0" id="svg_4" y="66.28754" x="75.82915" fill="#000000">JS</text>
